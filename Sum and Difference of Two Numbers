#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int s1,s2;              //create variable both int,float.
    float a1,a2;
    scanf("%d%d",&s1,&s2);
    printf("%d %d",(s1+s2),(s1-s2));
    printf("\n");
    scanf("%f%f",&a1,&a2);
    printf("%.01f %.01f",(a1+a2),(a1-a2));        //%.01f for print only one point values like 1.0
    
    return 0;
}
